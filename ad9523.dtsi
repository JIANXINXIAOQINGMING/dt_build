&spi0 {
	status = "okay";
};

//#define fmc_spi spi0

#include <ad9528.h>

/ {
	clocks {

	};
};

&spi0 {
	clkgen: ad9545 {
			#address-cells = <1>;
			#size-cells = <0>;
			compatible = "ad9548";
			reg = <0>;
			spi-3wire;
			spi-max-frequency = <10000000>;

		ad9545_0_c0: ad9545_name1 {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <25000012>;
		};
		ad9548_0_c1: ad9545_name2 {
			#clock-cells = <0>;
			compatible = "fixed-clock";
			clock-frequency = <25000034>;
		};
	};

};
